/* component/main/main.wxss */
/* px%2*2 */
.main{
  width: 750rpx;
  height: 900rpx;
  overflow: hidden;
}
.bg{
  position: relative;
}
.box{
  position: absolute;
  top: 0;
  left: 50%;
  transform: translateX(-50%);
  text-align: center;
}
.wendu{
  font-size:194rpx;
  font-weight:normal;
  color:rgba(255,255,255,1);
  position: relative;
}
.wendu::after{
  content: '';
  display: inline-block;
  position: absolute;
  top:15%;
  right: -10%;
  width: 48rpx;
  height: 42rpx;
  background-image: url('data:image/png;base64,');
  background-size: 100% 100%;
  background-repeat: no-repeat;
}
.text{
  display: block;
height:40rpx;
font-size:42rpx;
font-weight:bold;
color:rgba(255,255,255,1);
line-height:106rpx;
}
.qujian{
  display: block;
  box-sizing: border-box;
  margin-top: 41rpx;
height:36rpx;
font-size:42rpx;
font-weight:400;
color:rgba(255,255,255,1);
line-height:110rpx;
}

