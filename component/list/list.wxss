/* component/list/list.wxss */
.list{
  margin-top: 56rpx;
  width: 100%;
  padding: 0 40rpx;
  box-sizing: border-box;
  font-size: 24rpx;
  display: flex;
  justify-content: space-around;
  /* overflow: hidden; */
}
.list-item{
  flex: 1;
  text-align: center;
}
.list-item-day{
  height:24rpx;
font-size:26rpx;
font-weight:bold;
  color:rgba(0,0,0,1);
}
.list-item-icon{
  box-sizing: border-box;
  margin: 0 auto;
  margin-top: 52rpx;
  width: 78rpx;
  height: 78rpx;
}
.list-item-qujian{
  margin-top: 40rpx;
  box-sizing: border-box;
font-size:26rpx;
font-weight:normal;
color:rgba(0,0,0,1);
line-height:70rpx;
}
